@model FossTechCrm.Entities.Lead

<h2>Delete Employee</h2>

@using (Html.BeginForm("delete", "home", FormMethod.Post, new { id = "popupForm" }))

{
    @Html.HiddenFor(a => a.LeadID)
    <div class="form-group">
        <label>First Name</label>
        <p>@Model.FirstName</p>
    </div>
    <div class="form-group">
        <label>Description</label>
        <p>@Model.CompanyName</p>
    </div>
    <div class="form-group">
        <label>Description</label>
        <p>@Model.Description</p>
    </div>
    <div class="form-group">
        <label>Email</label>
        <p>@Model.Email</p>
    </div>
    <div class="form-group">
        <label>LeadSource</label>
        <p>@Model.LeadSource</p>
    </div>
    <div class="form-group">
        <label>Status</label>
        <p>@Model.Phone</p>
    </div>
    <div class="form-group">
        <label>Status</label>
        <p>@Model.Ratings</p>
    </div>
    <div class="form-group">
        <label>Status</label>
        <p>@Model.Status</p>
    </div>

    <input type="submit" value="Submit" />
}